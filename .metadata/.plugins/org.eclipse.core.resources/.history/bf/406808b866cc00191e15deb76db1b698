package com.deepak.passengerService;

import java.util.ArrayList;
import java.util.List;

import javax.ws.rs.FormParam;

import org.springframework.stereotype.Service;

@Service
public class PassengerServiceImple implements PassengerService {

	List<Passenger> passengers=new ArrayList<>();
	int currentId=123;

	@Override
	public List<Passenger> getPassengers(int start, int size) {
		System.out.println("Start: "+start);
		System.out.println("Size: "+size);

		return passengers;
	}

	@Override
	public void addPassenger(@FormParam("fn")String fn, @FormParam("ln")String ln) {
		
		Passenger p=new Passenger();
		p.setName(fn+ln);
		passengers.add(p);
		System.out.println(fn+ln);
	}


}
