package com.deepak.restws.model;

import java.util.List;

import javax.ws.rs.GET;
import javax.ws.rs.Path;

@Path("/patientService")
public interface PatientService {

	
	@Path("/patients")
	@GET
	public List<Patient> getPatients();
	
	@Path("/patients/{id}")
	public Patient getPatient(long id);
	
}
